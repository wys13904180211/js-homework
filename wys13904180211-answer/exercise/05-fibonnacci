/*2016211765 王宇晟 05*/
var x = 1;
var n;

n = parseInt(prompt("输入 n,n>=1", " "));
function fib(n)
{
    var a = 1;
    var b = 1;
    if (n == 1 || n == 2)
        x = 1;
    else
    {
        for (var i = 3; i < n + 1; i++)
        {
            x = a + b;
            a = b;
            b = x;
        };
    };
    return (x);
};

x = fib(n);
document.write("f(" + n + ")=" + x);
